# Server Configuration
# Copy this file to .env and fill in your values

# Server Settings
PORT=8767
HOST=0.0.0.0

# Security
API_KEY_PREFIX=lb_

# Timeouts (in seconds)
SESSION_TIMEOUT=3600    # 1 hour
TUNNEL_TIMEOUT=300      # 5 minutes  
REQUEST_TIMEOUT=30      # 30 seconds

# API Keys (add your users here)
# Format: API_KEY_<username>=<api_key>
# Example:
# API_KEY_ALICE=lb_abcdef123456789
# API_KEY_BOB=lb_xyz987654321

# For testing (remove in production)
API_KEY_TEST_USER=lb_test_key_123
