# MCP Tunnel Client Configuration
# Run this on your local machine

# Bridge server WebSocket URL
BRIDGE_URL=ws://146.190.120.44:8767/tunnel/connect

# Local MCP server URL
LOCAL_MCP_URL=http://127.0.0.1:8766

# Local MCP API key (matches your MCP server config)
LOCAL_API_KEY=dev-key-local-only

# Your unique user identifier
# Generate with: python3 -c "import uuid; print(str(uuid.uuid4()))"
USER_ID=

# API key for external tools to access your remote endpoint
# Generate with: python3 -c "import secrets; print('lb_' + secrets.token_urlsafe(32))"
REMOTE_API_KEY=

# Keepalive interval (seconds)
KEEPALIVE_INTERVAL=30

# SSL verification (set to false for self-signed certs)
SSL_VERIFY=false
